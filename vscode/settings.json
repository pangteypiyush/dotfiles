{
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.folding": false,
    "editor.fontFamily": "'JetBrains Mono', 'Source Code Pro'",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.minimap.enabled": true,
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "all",
    "editor.scrollBeyondLastLine": false,
    "editor.stickyScroll.enabled": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "gitlens.blame.avatars": false,
    "gitlens.blame.highlight.enabled": false,
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.graph.minimap.additionalTypes": [
        "localBranches",
        "stashes",
        "tags"
    ],
    "gitlens.hovers.currentLine.changes": false,
    "gitlens.hovers.currentLine.details": false,
    "gitlens.hovers.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.statusBar.reduceFlicker": true,
    "go.toolsManagement.autoUpdate": true,
    "go.useLanguageServer": true,
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.latex.recipes": [
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "latexmk",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "args": [
                "-output-directory=%OUTDIR%",
                "%DOC%"
            ],
            "command": "xelatex",
            "env": {},
            "name": "xelatex"
        }
    ],
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "'JetBrains Mono', 'Source Code Pro', 'SauceCodePro Nerd Font'",
    "vim.camelCaseMotion.enable": true,
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "h",
                "n"
            ],
            "commands": [
                "editor.action.dirtydiff.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "h",
                "p"
            ],
            "commands": [
                "editor.action.dirtydiff.previous"
            ]
        },
        {
            "before": [
                "<leader>",
                "A",
                "n"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "A",
                "p"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "n"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "p"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "t"
            ],
            "commands": [
                "outline.focus"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "t"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "after": [
                "g",
                "c",
                "c"
            ],
            "before": [
                "<leader>",
                "x"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "r"
            ],
            "commands": [
                "testing.runAtCursor"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "testing.runCurrentFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "a"
            ],
            "commands": [
                "testing.runAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "d"
            ],
            "commands": [
                "testing.debugTestAtCursor"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "b"
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "i"
            ],
            "commands": [
                "references-view.findImplementations"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "r"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
    ],
    "vim.startofline": false,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "after": [
                "g",
                "c",
                "c"
            ],
            "before": [
                "<leader>",
                "x"
            ]
        },
    ],
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.editor.empty.hint": "hidden",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile"
}
