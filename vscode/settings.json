{
    "editor.accessibilitySupport": "off",
    "editor.cursorSmoothCaretAnimation": "explicit",
    "editor.folding": false,
    "editor.fontFamily": "JetBrains Mono, SauceCodePro NF",
    "editor.fontLigatures": true,
    "editor.lineNumbers": "relative",
    "editor.quickSuggestions": {
        "comments": "inline"
    },
    "editor.renderFinalNewline": "off",
    "editor.renderWhitespace": "all",
    "editor.screenReaderAnnounceInlineSuggestion": false,
    "editor.smoothScrolling": true,
    "editor.stickyScroll.maxLineCount": 1,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "explorer.confirmDragAndDrop": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.mergeEditor": true,
    "git.openRepositoryInParentFolders": "never",
    "go.editorContextMenuCommands": {
        "fillStruct": true
    },
    "go.inlayHints.functionTypeParameters": true,
    "go.inlayHints.rangeVariableTypes": true,
    "go.toolsManagement.autoUpdate": true,
    "redhat.telemetry.enabled": false,
    "remote.autoForwardPortsSource": "hybrid",
    "rust-analyzer.cargo.sysroot": "discover",
    "rust-analyzer.inlayHints.reborrowHints.enable": "always",
    "rust-analyzer.testExplorer": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "JetBrains Mono, SauceCodePro NF",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.showExitAlert": false,
    "terminal.integrated.splitCwd": "workspaceRoot",
    "todo-tree.filtering.ignoreGitSubmodules": true,
    "todo-tree.general.showActivityBarBadge": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.backgroundColourScheme": [
        "#fb4934",
        "#fe8019",
        "#cc241d",
        "#fabd2f",
        "#458588",
        "#665c54",
        "#928374"
    ],
    "todo-tree.highlights.foregroundColourScheme": [
        "#fbf1c7"
    ],
    "todo-tree.highlights.useColourScheme": true,
    "todo-tree.ripgrep.ripgrep": "/usr/bin/rg",
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c",
                "l"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "h",
                "n"
            ],
            "commands": [
                "editor.action.dirtydiff.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "h",
                "p"
            ],
            "commands": [
                "editor.action.dirtydiff.previous"
            ]
        },
        {
            "before": [
                "<leader>",
                "A",
                "n"
            ],
            "commands": [
                "editor.action.marker.nextInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "A",
                "p"
            ],
            "commands": [
                "editor.action.marker.prevInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "n"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "<leader>",
                "a",
                "p"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        {
            "before": [
                "<leader>",
                "t",
                "t"
            ],
            "commands": [
                "outline.focus"
            ]
        },
        {
            "before": [
                "<leader>",
                "n",
                "t"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "after": [
                "g",
                "c",
                "c"
            ],
            "before": [
                "<leader>",
                "x"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "r"
            ],
            "commands": [
                "test-explorer.run-this-test"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "test-explorer.run-this-file"
            ]
        },
        {
            "before": [
                "<leader>",
                "r",
                "a"
            ],
            "commands": [
                "test-explorer.run-all"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "d"
            ],
            "commands": [
                "test-explorer.debug-this-test"
            ]
        },
        {
            "before": [
                "<leader>",
                "d",
                "b"
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "b",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditorInGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "i"
            ],
            "commands": [
                "references-view.findImplementations"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "r"
            ],
            "commands": [
                "references-view.findReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "c"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "g",
                "t"
            ],
            "commands": [
                "workbench.action.focusActiveEditorGroup"
            ]
        }
    ],
    "vim.replaceWithRegister": true,
    "vim.showMarksInGutter": true,
    "vim.startofline": false,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "after": [
                "g",
                "c",
                "c"
            ],
            "before": [
                "<leader>",
                "x"
            ]
        }
    ],
    "window.menuBarVisibility": "toggle",
    "workbench.colorCustomizations": {
        "[Gruvbox Dark Hard]": {
            "editorInlayHint.background": "#00000040",
            "editorInlayHint.foreground": "#978774"
        }
    },
    "workbench.colorTheme": "Gruvbox Dark Hard",
    "workbench.editor.alwaysShowEditorActions": true,
    "workbench.editor.empty.hint": "hidden",
    "workbench.sideBar.location": "right",
    "zig.checkForUpdate": false,
    "zig.formattingProvider": "zls",
    "zig.initialSetupDone": true,
    "zig.zls.checkForUpdate": false,
    "workbench.layoutControl.enabled": false,
    "window.commandCenter": false
}
